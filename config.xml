<?xml version='1.0' encoding='utf-8'?>
<widget id="com.garstapp.v1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Garst App</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="info@garstapp.com" href="http://garstapp.com">
        Covalent Social Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-facebook4" spec="~1.7.4">
        <variable name="APP_ID" value="222222222222222" />
        <variable name="APP_NAME" value="social.covalent.project-w" />
    </plugin>
</widget>
